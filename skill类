
public class skii {
public  static String skill(int a,int b) {
	String[] skill = {
  //这里放入你的技能库，技能的概率可以通过调整技能的相对数量进行决定
  };
	if(skill[a].toString()==skill[b].toString()) {b=b+1;}
	String ss1 = "②"+skill[a];
